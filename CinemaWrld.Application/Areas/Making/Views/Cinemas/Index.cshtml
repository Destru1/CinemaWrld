@model CinemaWrld.Application.Areas.Making.Models.Cinemas.ViewModels.CinemasViewModel
@{}



<div>
    <a class="btn btn-outline-primary" asp-action="Create">Add cinema</a>
</div>


<div>
    <ul>
    @foreach (var cinema in Model.Cinemas)
    {
      <li>@cinema.Name | @cinema.Location | @cinema.PhoneNumber</li>
    }
        </ul>
</div>